#! /usr/bin/env node /usr/local/bin/jtree
test compile
 createNewBlankProgramCommand hello-world.fire
  assertStringIncludes getTextContent hello-world
  pasteCommand
   withParagraph
    set$ greeting Hello world
    log$ greeting
   assertLengthIs getMountedTilesProgram.getTiles 2
   saveCompiledCommand
test compile2
 createNewBlankProgramCommand hello-world.fire
  assertStringExcludes getTextContent Ohayo!
  pasteCommand
   withParagraph
    tiles.picker
     pin 8 4
   assertLengthIs getRenderedTilesDiagnostic 1
   assertStringIncludes getTextContent hello-world
   assertStringExcludes getTextContent errors
   assertStringExcludes getTextContent Ohayo!
   cellCheckProgramCommand
    assertStringIncludes getTextContent errors
test executeProgramCommand
 createNewBlankProgramCommand hello-world.fire
  clearTabMessagesCommand
   executeProgramCommand
    createNewSourceCodeVisualizationProgramCommand
     assertStringIs getMountedTab.getFileName hello-world.fire-source-code-vis.maia