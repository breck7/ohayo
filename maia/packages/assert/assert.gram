 javascript
  async execute() {
    const num = this.getWord(1)
    if (!num)
      return super.execute()
    const expected = parseInt(num)
    const actual = this.getParentOrDummyTable().getRowCount()
    if (actual !== expected)
      throw new Error(`Expected ${expected} but got ${actual}`)
    return super.execute()
  }