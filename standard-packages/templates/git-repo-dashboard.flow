html.h1 Statistics for Local Git Repo
web.get http://localhost:2222/shell?command=gitlog&cwd=/aientist/
 date.addColumns
  group.by day
   date.heatcal
    title Commits by day
    count count
   show.median count Median Commits Per Coding Day
   show.count count # Coding Days
  group.by month
   vega.line
    title Monthly Commit Trends
    xColumn month
    yColumn count
   vega.bar
    xColumn month
    yColumn count
    title Days worked by month
 show.count author Total Commits
 show.max time Most Recent Commit
 show.min time First Commit
layout bin