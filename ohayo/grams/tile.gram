   const hakonCode = this.hakonTemplate ? new jtree.TreeNode(theme).evalTemplateString(this.hakonTemplate) : this.toHakonCode()
   return `${selector} { ${visibleCss} ${dimensionCss} }
        ${theme.hakonToCss(hakonCode)}`
