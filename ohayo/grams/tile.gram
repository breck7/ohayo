  async execute() {
   try {
    this.setRunTimePhaseError("execute")
    await this._execute()
   } catch (err) {
    this.setRunTimePhaseError("execute", err)
    console.error(err)
    this.emitLogMessage(this.errorLogMessageStumpTemplate)
   }
   return this
  }
